{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "municipality": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "last_updated": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "zoningCodes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "dateAdopted": {
            "type": "string"
          },
          "districtTypes": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "status": {
                  "type": "string"
                },
                "definesUse": {
                  "type": "integer"
                }
              }
            }
          },
          "districtsHeirarchy": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "districts": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                }
              }
            }
          },
          "dataAdopted": {
            "type": "string"
          }
        }
      }
    }
  }
}
